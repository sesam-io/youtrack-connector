{
  "_id": "{{@ system @}}",
  "headers": {
    "Authorization": "Bearer $SECRET(api_key)"
  },
  "operations": {
    "projects-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,name,shortName"
      },
      "url": "admin/projects"
    },
    "users-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,avatarUrl,banned,email,fullName,guest,jabberAccountName,login,online,profiles,ringId,savedQueries,tags"
      },
      "url": "users"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://{{@ subdomain @}}.youtrack.cloud/api/%s",
  "verify_ssl": true
}
