{
  "_id": "{{@ system @}}",
  "headers": {
    "Authorization": "Bearer $SECRET(api_key)"
  },
  "operations": {
    "issues-delete": {
      "method": "DELETE",
      "url": "api/issues/{{ id }}"
    },
    "issues-insert": {
      "method": "POST",
      "url": "api/issues"
    },
    "issues-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,attachments(id),comments(id),created,description,externalIssue(id),isDraft,links(id),parent(id),project(id),resolved,subtasks(id),reporter(id),updated"
      },
      "url": "api/issues"
    },
    "issues-lookup": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,attachments(id),comments(id),created,description,externalIssue(id),isDraft,links(id),parent(id),project(id),resolved,subtasks(id),reporter(id),updated"
      },
      "url": "api/issues/{{ id }}"
    },
    "issues-update": {
      "method": "POST",
      "url": "api/issues/{{ id }}"
    },
    "projects-delete": {
      "method": "DELETE",
      "url": "api/admin/projects/{{ id }}"
    },
    "projects-insert": {
      "method": "POST",
      "url": "api/admin/projects"
    },
    "projects-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "next_page_link": "{{url}}",
      "next_page_termination_strategy": [
        "empty-result"
      ],
      "params": {
        "$skip": "{% if is_first_page  %}0{% else %}{{previous_params['$top'] * page}}{% endif %}",
        "$top": 42,
        "fields": "id,name,shortName,archived,createdBy(id),description,iconUrl,leader(id),team(id),template"
      },
      "url": "api/admin/projects"
    },
    "projects-lookup": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,name,shortName,archived,createdBy(id),description,iconUrl,leader(id),team(id),template"
      },
      "url": "api/admin/projects/{{ id }}"
    },
    "projects-update": {
      "method": "POST",
      "url": "api/admin/projects/{{ id }}"
    },
    "users-delete": {
      "method": "DELETE",
      "url": "hub/rest/users/{{ id }}"
    },
    "users-insert": {
      "method": "POST",
      "url": "hub/rest/users"
    },
    "users-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "next_page_link": "{{url}}",
      "next_page_termination_strategy": [
        "empty-result"
      ],
      "params": {
        "$skip": "{% if is_first_page  %}0{% else %}{{previous_params['$top'] * page}}{% endif %}",
        "$top": 50,
        "fields": "id,login,profile,banned,aliases,name"
      },
      "payload_property": "users",
      "url": "hub/rest/users"
    },
    "users-lookup": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "params": {
        "fields": "id,login,profile,banned,aliases,name"
      },
      "url": "hub/rest/users/{{ id }}"
    },
    "users-update": {
      "method": "POST",
      "url": "hub/rest/users/{{ id }}"
    },
    "usersyoutrack-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "next_page_link": "{{url}}",
      "next_page_termination_strategy": [
        "empty-result"
      ],
      "params": {
        "$skip": "{% if is_first_page  %}0{% else %}{{previous_params['$top'] * page}}{% endif %}",
        "$top": 50,
        "fields": "id,login,ringId"
      },
      "url": "api/users"
    },
    "workitems-list": {
      "id_expression": "{{ id }}",
      "method": "GET",
      "next_page_link": "{{url}}",
      "next_page_termination_strategy": [
        "empty-result"
      ],
      "params": {
        "$skip": "{% if is_first_page  %}0{% else %}{{previous_params['$top'] * page}}{% endif %}",
        "$top": 42,
        "fields": "id,author(id),creator(id),text,type(id),created,updated,duration(id,minutes,presentation),date,issue(id),attributes(id,value(id))"
      },
      "url": "api/workItems"
    }
  },
  "type": "system:rest",
  "url_pattern": "https://{{@ subdomain @}}.youtrack.cloud/%s",
  "verify_ssl": true
}